{
    "private": true,
    "scripts": {
        "preinstall": "node ./preinstall.js",
        "generate-routes": "php artisan ziggy:generate resources/assets/shared/js/routes.generated.js --url=${APP_ADMIN_DOMAIN}",
        "front-mix": "cross-env process.env.application=front mix",
        "front-dev": "yarn front-mix",
        "front-watch": "yarn front-mix watch",
        "front-watch-poll": "yarn front-watch --watch-options-poll=1000",
        "front-hot": "yarn front-watch --hot",
        "front-prod": "yarn front-mix --production",
        "auth-mix": "cross-env process.env.application=auth mix",
        "auth-dev": "yarn auth-mix",
        "auth-watch": "yarn auth-mix watch",
        "auth-watch-poll": "yarn auth-watch --watch-options-poll=1000",
        "auth-hot": "yarn auth-watch --hot",
        "auth-prod": "yarn auth-mix --production",
        "admin-mix": "cross-env process.env.application=admin mix",
        "admin-dev": "yarn admin-mix",
        "admin-watch": "yarn admin-mix watch",
        "admin-watch-poll": "yarn admin-watch --watch-options-poll=1000",
        "admin-hot": "yarn admin-watch --hot",
        "admin-prod": "yarn admin-mix --production",
        "prod": "yarn front-prod && yarn auth-prod && yarn admin-prod"
    },
    "devDependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
        "@vue/babel-preset-jsx": "^1.1.2",
        "babel-eslint": "^10.1.0",
        "cross-env": "^7.0",
        "eslint": "^7.17.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^5.0.0",
        "eslint-plugin-vue": "^7.4.1",
        "laravel-mix": "^6.0.6",
        "postcss": "^8.2.2",
        "postcss-import": "^14.0.0",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.32.0",
        "sass-loader": "^10.1.0",
        "tailwind-config-viewer": "^1.4.0",
        "vue-loader": "^15.9.5",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.32",
        "@fortawesome/free-solid-svg-icons": "^5.15.1",
        "@fortawesome/vue-fontawesome": "^2.0.2",
        "@inertiajs/inertia": "^0.8.2",
        "@inertiajs/inertia-vue": "^0.5.4",
        "@inertiajs/progress": "^0.2.4",
        "@tailwindcss/ui": "^0.7.2",
        "apexcharts": "^3.25.0",
        "axios": "^0.21.0",
        "bikram-sambat-js": "^1.0.1",
        "laravel-jetstream": "^1.2.0",
        "lodash": "^4.17.19",
        "nepali-number": "^1.0.3",
        "portal-vue": "^2.1.7",
        "qs": "^6.9.4",
        "tailwindcss": "^2.0.2",
        "vue": "^2.5.17",
        "vue-apexcharts": "^1.6.0",
        "vue-fragment": "^1.5.1",
        "vue-multiselect": "^2.1.6",
        "vue-slick-carousel": "^1.0.6"
    }
}
